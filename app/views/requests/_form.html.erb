<%= simple_form_for @request, url: requests_path, method: :post do |f| %>
  <%= f.input :requester, input_html: { class:'form-control' }, wrapper_html: { class: 'm-3' } %>
  <%= f.input :music_id, as: :select, collection: Music.collection(Music.all),  input_html: { class:'form-control js-searchable' }, wrapper_html: { class: 'm-3' } %>
  <br>
  <%= f.button :submit %>
<% end %>